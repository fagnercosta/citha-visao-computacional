   "source": [
    "import cv2\n",
    "captura = cv2.VideoCapture(0) \n",
    "while captura.isOpened():\n",
    "    ret, frame = captura.read()\n",
    "    cv2.imshow(\"Imagem\", frame)\n",
    "    \n",
    "    if cv2.waitKey(1) & 0xFF == ord(\"q\"):\n",
    "        break\n",
    "captura.release()\n",
    "cv2.destroyAllWindows()"
   ]
