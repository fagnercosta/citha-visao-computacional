    "#Importando a biblioteca do OpenVC\n",
    "import cv2 as cv\n",
    "#Lendo a imagem no disco\n",
    "imagem = cv.imread(\"IFAM.png\")\n",
    "#Apresentando a imagem\n",
    "cv.imshow(\"Lendo Imagem\", imagem)\n",
    "#Definindo tecla para fechar a janela\n",
    "cv.waitKey(0 ) \n",
    "#Chefando a janela\n",
    "cv.destroyAllWindows()\n",
    "\n"
   ]
